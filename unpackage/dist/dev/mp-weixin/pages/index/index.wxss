.home {
  width: 750rpx;
  background-image: url('data:image/png;base64,');
  background-size: 938rpx auto;
  background-position: center -64rpx;
  background-repeat: no-repeat;
}
.home .top {
  height: 70rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  position: -webkit-sticky;
  position: sticky;
  top: 100rpx;
  z-index: 999;
  background-image: url('data:image/png;base64,');
  background-size: 938rpx auto;
  background-position: center -64rpx;
  background-repeat: no-repeat;
}
.home .top .scan {
  width: 106rpx;
  display: flex;
  justify-content: space-around;
}
.home .top .scan .icon-scan {
  font-size: 40rpx;
}
.home .top .search {
  flex: auto;
}
.home .top .search input {
  height: 60rpx;
  border-radius: 30rpx;
  font-size: 22rpx;
  background-color: #fff;
  padding: 0 20rpx;
  line-height: 60rpx;
}
.home .top .msg {
  width: 106rpx;
  display: flex;
  justify-content: space-around;
}
.home .top .msg .icon-msg {
  font-size: 40rpx;
}
.home .bottom {
  width: 702rpx;
  height: 67rpx;
  color: #fff;
  font-size: 28rpx;
  font-weight: 500;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-around;
  align-items: flex-start;
  margin: 30rpx auto;
}
.home .bottom .header-tab {
  flex: auto;
  white-space: nowrap;
  overflow-x: auto;
  scrollbar-width: 0;
}
.home .bottom .header-tab .header-tab-item {
  width: 112rpx;
  display: inline-block;
  margin-right: 20rpx;
  text-align: center;
}
.home .bottom .header-tab-line {
  width: 40rpx;
  height: 20rpx;
  position: relative;
  bottom: 0;
  left: var(--left);
  border-bottom: 4rpx solid #fff;
  transition-property: left;
  transition-duration: 200ms;
}
.home .bottom .header-category {
  width: 160rpx;
  padding-left: 20rpx;
  display: flex;
  align-items: center;
}
.home .bottom .header-category .icon-menu {
  font-size: 30rpx;
}
.home .bottom .header-category text {
  margin-left: 20rpx;
}
.home .body {
  width: 700rpx;
  margin: 0 auto;
}
.home .body .banner {
  border-radius: 6rpx;
  overflow: hidden;
}
.home .body .banner .swiper {
  width: 702rpx;
  height: 320rpx;
}
.home .body .banner .swiper .swiper-item image {
  width: 702rpx;
  height: 320rpx;
}
.home .body .safeguard {
  height: 9999rpx;
}
